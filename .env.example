# SEO MCP Client Configuration
# Copy this file to .env and add your API keys

# CapSolver API Key (for CAPTCHA solving)
CAPSOLVER_API_KEY=your_capsolver_api_key_here

# Ahrefs API Key (if you have Ahrefs subscription)
AHREFS_API_KEY=your_ahrefs_api_key_here

# SEMrush API Key (if you have SEMrush subscription)
SEMRUSH_API_KEY=your_semrush_api_key_here

# Optional: Custom MCP Server endpoints
MCP_SERVER_URL=http://localhost:8000

# Cache settings
CACHE_EXPIRY_HOURS=1
REQUEST_TIMEOUT_MS=30000